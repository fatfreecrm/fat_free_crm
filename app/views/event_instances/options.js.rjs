page.call "crm.flick", :empty, :toggle
page.call "crm.hide_form", :create_event_instance
page.call "crm.flip_form", :options

unless params[:cancel].true?
  page[:options].replace_html :partial => "options"
  page.call "crm.set_title", :create_event_instance, t(:event_instances_options)
else
  page.call "crm.set_title", :create_event_instance, t(:event_instances)
end
