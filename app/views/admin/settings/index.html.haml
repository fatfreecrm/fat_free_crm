.title= t(:admin_tab_settings)

= form_for "settings", url: admin_settings_path, method: :put do |f|
  = render partial: "application", locals: { f: f }
  = render partial: "user_signup", locals: { f: f }
  = render partial: "email", locals: { f: f }
  = render partial: "validations", locals: { f: f }
  = render partial: "customization", locals: { f: f }
  = render partial: "ai_prompts", locals: { f: f }
  .form-group
    = f.submit t(:update), class: "btn btn-primary"
