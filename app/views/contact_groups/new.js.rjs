page.call "crm.flick", :empty, :toggle
#page.call "crm.hide_form", :options if called_from_index_page?
page.call "crm.flip_form", :create_contact_group

unless params[:cancel].true?
  page[:create_contact_group].replace_html :partial => "new"
  page.call "crm.init_chosen_fields"
  page.call "crm.set_title", :create_contact_group, t(:create_contact_group)
else
  page.call "crm.set_title", :create_contact_group, t(:contact_groups)
end
